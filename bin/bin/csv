#!/usr/bin/env python

from __future__ import print_function

import csv
import sys

selector = sys.argv[1]
if selector.isdigit():
    i = int(selector)
    reader = csv.reader(sys.stdin)
    for row in reader:
        if len(row) >= i:
            print(row[i])
else:
    reader = csv.DictReader(sys.stdin)
    for row in reader:
        if selector in row:
            print(row[selector])
