#!/usr/bin/env python

import collections, operator, sys

lines = collections.defaultdict(int)
total = 0
for line in sys.stdin:
    lines[line] += 1
    total += 1

for line, count in sorted(lines.items(), key=operator.itemgetter(1), reverse=True):
    if len(sys.argv) >= 2 and sys.argv[1] == '-p':
        count = 1.0 * count * 100 / total
        count = '%0.1f' % count
    print "%6s %s" % (count, line),

if len(sys.argv) < 2 or sys.argv[1] != '-p':
    print "%6s %s" % (total, 'TOTAL')
