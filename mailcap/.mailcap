# Note regarding %s in different contexts:
#   In test= context, %s is the filename specified in the MIME header.  If there
#   is no name specified, then %s will resolve to blank (and will not even be
#   quoted as an empty argument by mutt).  For example:
#       Content-Type: application/x-msdownload; name="pack276.exe"
#   
#   In viewer context, %s is the temporary filename created by mutt.  It will
#   almost certainly be different from the name used in test= context since it
#   will be a temporary file.  Unless nametemplate= is specified, the extension
#   of the temporary file will not be significant and should be ignored by the
#   viewer.  Additionally %s should never be blank in viewer context.
#
# Refer to for reference only:
#   w=`(stty size </dev/tty||echo '24 80') | cut -d' ' -f2 | xargs expr -10 +` && \
#   ... \
#   | sed 's/[[:blank:]]*$//' | cat -s

text/html; mailcap_bg %s browser file://@s ; nametemplate=%s.html ; test=test -n "$DISPLAY" && which browser >/dev/null 2>&1
text/html; mailcap_bg %s chromium-browser file://@s ; nametemplate=%s.html ; test=test -n "$DISPLAY" && which chromium-browser >/dev/null 2>&1
text/html; mailcap_bg %s firefox file://@s ; nametemplate=%s.html ; test=test -n "$DISPLAY" && which firefox >/dev/null 2>&1
text/html; mailcap_bg %s mozilla file://@s ; nametemplate=%s.html ; test=test -n "$DISPLAY" && which mozilla >/dev/null 2>&1
text/html; w3m    %s ; nametemplate=%s.html ; needsterminal ; test=which w3m    >/dev/null 2>&1
text/html; links2 %s ; nametemplate=%s.html ; needsterminal ; test=which links2 >/dev/null 2>&1
text/html; elinks %s ; nametemplate=%s.html ; needsterminal ; test=which elinks >/dev/null 2>&1
text/html; links  %s ; nametemplate=%s.html ; needsterminal ; test=which links  >/dev/null 2>&1
text/html; fix-html %s | env RAN_HTML_FIXUPS=1 run-mailcap --action=cat text/html:- ; \
           copiousoutput ; test=test -z "$RAN_HTML_FIXUPS" && which fix-html >/dev/null 2>&1
text/html; w=`(stty size </dev/tty||echo '24 80') | cut -d' ' -f2 | xargs expr -10 +` && \
           w3m -T text/html -cols $w -o frame=1 -o graphic_char=1 -o ignore_null_img_alt=1 -o color=0 -o follow_locale=1 -dump %s ; \
           copiousoutput ; nametemplate=%s.html ; \
           print=w3m -dump -cols 132 -T text/html %s | mpage -bLetter -1 -P -M132 ; \
           test=which w3m >/dev/null 2>&1
text/html; w=`(stty size </dev/tty||echo '24 80') | cut -d' ' -f2 | xargs expr -10 +` && \
           c=`eval $(locale)\; case "$LC_CTYPE" in *.utf8) echo " -codepage iso-10646-1" \;\; esac` && \
           links2 $c -width $w -html-margin 0 -html-numbered-links 1 -force-html -dump %s | sed 's/[[:blank:]]*$//' | cat -s; \
           nametemplate=%s.html ; copiousoutput ; test=which links2 >/dev/null 2>&1
text/html; w=`(stty size </dev/tty||echo '24 80') | cut -d' ' -f2 | xargs expr -10 +` && \
           elinks -dump 1 -dump-width $w -localhost 1 -eval "set document.browse.margin_width = 0" %s ; \
           nametemplate=%s.html ; copiousoutput ; test=which elinks >/dev/null 2>&1
text/html; w=`(stty size </dev/tty||echo '24 80') | cut -d' ' -f2 | xargs expr -10 +` && \
           links -dump -width $w %s ; \
           nametemplate=%s.html ; copiousoutput ; test=which links >/dev/null 2>&1

text/calendar; vcalendar-filter %s 2>&1 ; copiousoutput ; test=which vcalendar-filter >/dev/null 2>&1

application/pdf; LC_ALL=C mailcap_bg %s acroread ; nametemplate=%s.pdf ; test=test -n "$DISPLAY" && which acroread >/dev/null 2>&1
application/pdf; LC_ALL=C mailcap_bg %s evince   ; nametemplate=%s.pdf ; test=test -n "$DISPLAY" && which evince   >/dev/null 2>&1
